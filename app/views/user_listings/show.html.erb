<p style="color: green"><%= notice %></p>

<%= render @user_listing %>

<div>
  <%= link_to "Back to listings", user_listings_path %>

  <% @user_listing.listing.reviews.each do |review| %>
    <div class="card">
      <div class="card-body">
        <strong><%= review.author %></strong>
        <p><%= review.text %></p>
      </div>
    </div>
  <% end %>
</div>
